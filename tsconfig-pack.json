{
    "compilerOptions": {
        "jsx": "react",
        "target": "es5",
        "module": "esnext",
        "declaration": true,
        "outDir": "dist",
        "strict": false,
        "lib": ["dom", "esnext"],
        "esModuleInterop": true,
        "strictNullChecks": true,
        "downlevelIteration": true,
        "plugins": [],
        "moduleResolution": "node",
        "emitDeclarationOnly": true
    },
    "exclude": ["dist", "src/tests"]
}
